
        // tipos de variaveis : Number = 5, 6, 18; String = "Ola"; Boolean = false true; Array[]; Object{}. Não existe char apenas usar string

        var x = 5; //usar o let por ser mais utilisado
        let y = 8;
        // const w = 9;

        /*Console.Write == Console.Log*/ 
        console.log(x)
        console.log(y)

        //Escreve o tipo da variavel
        console.log(typeof x)

        //Estilo do bool
        let logico = true
        console.log(logico)
        console.log(typeof logico)

        let nome1 = "joão"
        let nome2 = "maria"
        let nome3 = "jose"

        //Concatenação de string
        console.log(nome1 + " e " + nome2)

        //interpolação de string
        const nome = "Matheus"
        const idade = 30;
        const frase = `O nome dele é ${nome} e ele tem ${idade} anos`
        console.log(frase)

        // relações maior e menor
        console.log(5 > 2)
        console.log(-2 < 1)
        console.log(2 * 2)
    

        // ; opicional pra terminar linhas de comando. O professor falou que tem um caso que não é opcional que é quando vamos execut\ar varios comandos na mesma linha

        //tipos especiais ou numeros especiais
        let a = 12
        let b = "carlos"

        console.log(b * a) // caso not a number, por que carlos é uma string e 12 é número


        console.log(typeof (10 / 0)) // caso infinity, isto é um número
        console.log(typeof (-10 / 0)) // caso -infinity, isto é um número
        console.log(Number.MAX_VALUE * 2)


        console.log(8 === "8") // Cuidado mistura de string e int, use 3 sinais de =

        //Empty value que é null e
        let e = null // ISTO É UMA INFORMAÇÃO DO TIPO OBJECT
        console.log(e)
        console.log(typeof e)

        let d
        console.log(d)
        console.log(typeof(typeof d))// isto é uma string

        console.log(null === undefined) // false NÃO SÃO IGUAIS
        console.log(null == undefined) // true, porém não são totalmente iguais

        console.log(null == false)
        console.log(undefined == false)

